<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
	namespace="faceboardpro.Domain" assembly="faceboardpro">
  <class name="Photo" table="Photo"  lazy="false">
    <id name="Id" column="Id" unsaved-value="0">
      <generator class="native" />
    </id>
    <property name="PhotoId">
      <column name="PhotoId"  />
    </property>
    <property name="PhotoURL">

      <column name="PhotoURL"></column>
    </property>
    
  </class>
</hibernate-mapping>

